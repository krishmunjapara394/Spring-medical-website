{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": "off",
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select"
        ]
      }
    ],
    "axe/name-role-value": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ]
  }
}