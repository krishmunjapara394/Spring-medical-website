{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "position: sticky"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "link[referrerpolicy]"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ]
  }
}